function formValidationb()  
{  
var veg=document.registration.vegetable;
var wsr=document.registration.WS/Retailer;
var qreq=document.registration.qtyreq;
var units=document.registration.units;  
if(select(veg))  
{
if(select(wsr))  
{
if(isqreq(qreq))  
{
if(cselect(units)) 
{ 
}
}
}
}
return false;  
}

  

function isqreq(qreq)  
{   
var numbers = /^[0-9]+$/;  
if(qreq.value.match(numbers))  
{  
return true;  
}  
else  
{  
alert('quantity required must have numeric characters only');  
qreq.focus();  
return false;  
}  
} 
 
 
function select(veg)
{
 
   if( veg.value == "" || veg.value=="-1" )
   {
     alert( "Please select one of the type of vegetables" );
     veg.focus() ;
     return false;
   }
   
  
   return true ;
}
function select(wsr)
{
 
   if( wsr.value == "" || wsr.value=="-1" )
   {
     alert( "Please select one of the WholeSeller/Retailer" );
     wsr.focus() ;
     return false;
   }
   
  
   return true ;
}

function cselect(units)
{
 
   if( units.value == "" || units.value=="-1" )
   {
     alert( "Please select one of the units" );
     units.focus() ;
     return false;
   }
   
  
   return true ;
}</script>

