	width:771px;
}
#non_flash{
	background:url(/Assets/templates/non-flash.jpg) top left repeat-x;
	position:relative;
	text-align:center;
}
#flash_enabled{
	display:block;
	position:absolute;
	right:0px;
	bottom:0px;
	width:98px;
	height:26px;
}
