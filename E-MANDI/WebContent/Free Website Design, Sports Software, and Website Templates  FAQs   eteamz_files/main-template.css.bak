	padding-top:50px;
	margin:0px;
}
/* Breadcrumb */
#breadcrumb{
	background:url(/Assets/templates/breadcrumb-sprites.gif) 0px 0px no-repeat;
	color:#ffffff;
	font-family:Arial, Helvetica, sans-serif;
	font-size:11px;
	font-weight:bold;
